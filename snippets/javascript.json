{
	"it": {
		"prefix": "it",
		"body": "it( '' , function () {\n\n});",
		"description": "Función que contiene la llamada a los métodos",
		"scope": "js"
	},
	"jasmine.objectContaining" : {
		"prefix" : "jasmineObjectContaining",
		"body" : [
			"jasmine.objectContaining({ width: ${width}, height ${height}})"
		],
		"description" : "jasmine.objectContaining"
	},
	"describe" : {
		"prefix" : "describe",
		"body" : [
			"describe('$1', function() {",
			"\t${body}",
			"});"
		],
		"description" : "describe"
	},
	"beforeAll" : {
		"prefix" : "beforeAll",
		"body" : [
			"beforeAll( function (){",
			"\t${body}",
			"})"
		],
		"description" : "beforeAll"
	},
	"require" : {
		"prefix" : "require",
		"body" : [
			"require('${path}');"
		],
		"description" : "require"
	}
}