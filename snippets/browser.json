		"description" : "browser.addMockModule",
		"scope"  : "js"
   },
   "browser.clearMockModules":{
    	"prefix" : "brcm",
    	"body"   : "browser.clearMockModules($1);",
		"description" : "browser.clearMockModules",
		"scope"  : "js"
   },
