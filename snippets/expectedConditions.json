{
    "expect.not" : {
        "prefix" : "expectNot",
        "body" : [
            "expect(${element}).not(${expectedCondition})"
        ],
        "description" : "expect.not"
    },
    "expect.and" : {
        "prefix" : "expectAnd",
        "body" : [
            "var EC = protractor.ExpectedConditions;",
            "browser.wait(",
            "\tEC.and(${expect1}, ${expect2})",
            ", ${numberWait});"
        ],
        "description" : "expect.and"
    },
    "expect.alertIsPresent" : {
        "prefix" : "expectAlertIsPresent",
        "body" : [
            "var EC = protractor.ExpectedConditions;",
            "browser.wait(",
            "\tEC.alertIsPresent()",
            ", ${numberWait});"
        ],
        "description" : "expect.alertIsPresent"
    },
    "expect.elementToBeClickable" : {
        "prefix" : "expectElementToBeClickable",
        "body" : [
            "var EC = protractor.ExpectedConditions;",
            "browser.wait(",
            "\tEC.elementToBeClickable(${element})",
            ", ${numberWait});"
        ],
        "description" : "expect.elementToBeClickable"
    }
}